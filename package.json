{
  "name": "@panghui8383/flomo-mcp",
  "homepage": "https://github.com/SheldonFat/flomo-mcp",
  "version": "0.0.5",
  "description": "A Model Context Protocol server",
  "private": false,
  "type": "module",
  "bin": {
    "flomo-mcp": "./build/index.js"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "build": "tsc && node -e \"require('fs').chmodSync('build/index.js', '755')\"",
